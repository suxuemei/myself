body{
 background:url(../img/index/background.jpg) no-repeat;
 background-size:cover;
 height:4555px;
}
#header{
position:relative;
width:1100px;
min-height:330px;
margin:0 auto;
}
#bannerCaption{
  position:relative;
  margin:0 auto;
  left:105px;
  margin-top:-175px;
  margin-left:50px;
  height:415px;
  width:802px;
  box-sizing:border-box;
  border:1px solid red;
  overflow:hidden;
}
#bannerLists{
 position:absolute;
 width:800px;
 min-height:400px;
/* border:1px solid red;*/
}
#bannerImgs{
 position:absolute;
 top:0;
 left:0;
 height:400px;
 width:960px;
 box-sizing:border-box;
overflow:hidden;
}

#bannerImgs ul{
	position:absolute;
	display:flex;
}
#bannerImgs ul>li{
	width:960px;
    height:450px;
}
#bannerImgs img{
   display:block;
   width:960px;
   height:450px;
   border:1px solid transparent;
   box-sizing:border-box;
   box-shadow:2px 2px 5px #e7fafd;
}
#bannerLists>a{
  display:inline-block;
  position:absolute;
  width:20px;
  height:60px;
  border:1px solid transparent;
  border-radius:5px;
  background-color: #bababa;
  line-height:60px;
  text-align: center;
  color:#fff;
  top:160px;
}
#bannerLists>a:hover{
  background-color: lightblue;
}
#bannerLists>a.bannerHandL{
  left:10px;
}
#bannerLists>a.bannerHandR{
  right:10px;
}
#bannerLists>#bannerIndicators{
  position:absolute;
  left:35%;
  top:380px;
}
#bannerLists>#bannerIndicators ul{
   display:flex;
}
#bannerLists>#bannerIndicators li{
   width:50px;
   border:5px solid transparent;
   border-radius:3px;
   background-color:#eef;
   margin-right:10px;
}
#bannerLists>#bannerIndicators li:hover{
   background-color:#aaa;
}
#bannerLists>#bannerIndicators li.active{
   background-color:#aaa;
}
div#magic{
	position:relative;
	width:1100px;
	height:60px;
	/*left:155px;*/
	margin:0 auto;
	background:url(../img/index/p.gif) no-repeat;
	background-size:cover;
	box-shadow:5px 2px 5px pink;
	cursor:pointer;
	overflow:hidden;
}
div#magic p{
  position:absolute;
  /*overflow:hidden;*/
  left:500px;
  font:40px sans-serif;
  color:#d76797;
  line-height: 60px;
  text-align: center;
  /*border:1px solid red;*/
  box-sizing:border-box;
  text-shadow:25px 3px 5px gray;
}
div#magic>p:first-child{
   width:370px;
   height:60px;
   left:300px;
}
div#magic>p:nth-child(2){
  width:0;
  height:0;
  left:300px;
  overflow:hidden;
  color:pink;
  transition:all .001s;
  /*border:1px solid pink;*/
}
div#magic:hover p:nth-child(2){
  width:370px;
  height: 60px;
}
#imgIntr{
  position:absolute;
  width:290px;
  height:350px;
/*  border:1px solid red;*/
  left:820px;
  border-radius:3px;
  box-shadow:5px 5px 6px lightblue;
  transition:all 1.5s;
  z-index:0;
  padding:25px;
  text-align: center;
  line-height:20px;
  overflow:hidden;
}
#imgIntr ul{
	position:absolute;
	left:0;
	top:0;
	display:flex;
	overflow:hidden;
}
#imgIntr ul>li{
/*	border:1px solid red;*/
	width: 400px;
	height: 350px;
	 text-align: left;
	 padding:5px 0px;
	 margin-right:20px;
 box-sizing:border-box;
}
#imgIntr ul>li>span{
 font:25px sans-serif;
 color:lightblue;
 font-weight: bold;
 display:block;
 width:400px;
 height:30px;
 margin:20px 5px;
}
#imgIntr ul>li:first-child{
  display:block;
}
#imgIntr ul>li:nth-child(2){
  display:block;
}
#imgIntr ul>li:nth-child(3){
  display:block;
}
#imgIntr ul>li:nth-child(4){
  display:block;
}
#imgIntr:hover{
  transform:scale(1.05);
  z-index:10;
}
#classify{
  position:relative;
  top:25px;
  left:0;
  width:960px;
  min-height:1900px;
  margin:0 auto;
  /*border:1px solid red;*/
  text-align: center;
}
#classify>.diamond{
	position:absolute;
	left:0;top:0;
	width:960px;
	min-height:800px;
}
#classify .diaCaption{
   width:960px;
   height: 60px;
   /*border:2px solid purple;*/
}
#classify b{
   display:inline-block;
   position:absolute;
   width:100px;
   height:5px;
   background-color:lightblue;
   border:1px solid transparent;
   border-radius:6px;
   top:30px;
   transition:all 1s;
}
#classify b:first-child{
   left:260px;
}
#classify b:nth-child(2){
   left:600px;
}
#classify .diaCaption:hover  b:first-child{
    width:300px;
	left:60px;
}
#classify .diaCaption:hover  b:nth-child(2){
    width:300px;
}
#classify img{
  /*border:2px solid pink;*/
  width: 200px;
  height: 60px;
  transition:all 1s;
}
#classify  .diaBody{
  position:absolute;
  left:0;
  top:70px;
  width: 960px;
  height: 600px;
  /*border:1px solid red;*/
  border-radius:6px;
  }
#classify  .diaBody ul{
  display:flex;
  position:absolute;
  width: 960px;
  height: 600px;
  /*border:1px solid red;*/
}
#classify  .diaBody ul>li{
  position:absolute;
  width: 260px;height:260px;
  margin:20px 0;
  transition:all 1.5s;
  overflow:hidden;
  background-color: #fff;
  /*border:1px solid red;*/
}
#classify  .diaBody ul>li img{
 position:absolute;
}
#classify .diaBody ul>li:first-child,span{
  left:40px;
}
#classify  .diaBody ul>li:nth-child(2),span{
  left:340px;
}
#classify  .diaBody ul>li:nth-child(3),span{
  left:640px;
}
#classify  .diaBody ul>li:nth-child(4),span{
  left:40px;
  top:280px;
}
#classify  .diaBody ul>li:nth-child(5),span{
  left:340px;
  top:280px;
}
#classify  .diaBody ul>li:nth-child(6),span{
  left:640px;
  top:280px;
}
#classify  .diaBody ul>li>a{
  display:block;
  width: 260px;height: 260px;
  transition:all 1.5s;
}
#classify  .diaBody img{
  width: 260px;
  height: 260px;
}
#classify  .diaBody span{
  /*width:260px;
  height:260px;*/
  width:0;
  height:0;
  display:inline-block;
  opacity:.6;
  overflow:hidden;
  /*display:none;*/
  /*border:1px solid pink;*/
  background-color:#ebfaff;
  transition:all 1s;
}
#classify  .diaBody span ul{
  position:absolute;
  /*left:-40px;*/
}
#classify  .diaBody span li{
  text-align: center;
  position:absolute;
  /*width:260px;
  height:60px;*/
  width: 0;
  height: 0;
  transition:all .5s;
  line-height:60px;
  background-color:#ebfaff;
  /*border:1px solid red;*/
  margin-top:60px;
}
#classify  .diaBody span li:first-child{
  top:-20px;
  left:0;
  font:30px sans-serif;
  font-weight: bold;
  line-height:60px;
}
#classify  .diaBody ul>li img{
  left:0;
}
#classify  .diaBody span li:nth-child(2){
  top:40px;
  left:0;
}
#classify  .diaBody span li:nth-child(2)>b{
  width:5px;
  height:5px;
  position:absolute;
  left:125px;
  background-color:teal;
  transition:all 2s;
}
#classify  .diaBody span li:nth-child(3){
  top:100px;
  left:0;
  font:15px sans-serif;
  padding:0 15px;
}
#classify  .diaBody span li:nth-child(4){
  top:160px;
  left:0;
  height:50px;
  font:15px sans-serif;
  font-weight: bold;
  color:lightblue;
}
#classify  .diaBody>ul>li:hover{
  transform:scale(1.1);
}
#classify  .diaBody>ul>li:hover span{
   /*display:block;*/
   width: 260px;height: 260px;
}
#classify  .diaBody>ul>li span:hover li{
   /*display:block;*/
   width: 260px;height: 60px;
}
#classify  .diaBody>ul>li span:hover b{
   width:60px;
   left:100px;
}
#classify  .diaBody li:hover img{
  transform:rotate(-60deg);
}
